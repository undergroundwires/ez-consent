  "devDependencies": {
    "@babel/core": "^7.9.6",
    "babel-plugin-remove-import-export": "^1.1.1",
    "clean-css": "^4.2.3",
    "csslint": "^1.0.5",
    "google-closure-compiler": "^20200504.0.0",
    "lynt": "^0.5.5",
    "markdownlint-cli": "^0.23.0",
    "remark-cli": "^8.0.0",
    "remark-lint-no-dead-urls": "^1.0.2",
